{
  "name": "xld-microservice-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
    "start:dev": "nodemon",
    "build": "rm -rf dist && tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:create": "yarn typeorm migration:create",
    "typeorm:generate": "yarn typeorm migration:generate --dataSource src/loaders/dataSource.ts",
    "typeorm:show": "yarn typeorm migration:show --dataSource src/loaders/dataSource.ts",
    "typeorm:run": "yarn typeorm migration:run --dataSource src/loaders/dataSource.ts",
    "typeorm:revert": "yarn typeorm migration:revert --dataSource src/loaders/dataSource.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/logging-winston": "^5.1.2",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "helmet": "^5.1.0",
    "node-cron": "^3.0.1",
    "node-fetch": "^2.6.7",
    "pg": "^8.7.3",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node-cron": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
